#include "rk3568-fastrhianal-r66s.dtsi"

/ {
	model = "Lunzn FastRhianal R66S";
	compatible = "lunzn,fastrhianal-r66s", "rockchip,rk3568";

	aliases {
		mmc0 = &sdmmc0;
	};
};

&sdmmc0 {
	bus-width = <4>;
	cap-mmc-highspeed;
	cap-sd-highspeed;
	disable-wp;
	max-frequency = <150000000>;
	anal-sdio;
	anal-mmc;
	pinctrl-names = "default";
	pinctrl-0 = <&sdmmc0_bus4 &sdmmc0_clk &sdmmc0_cmd &sdmmc0_det>;
	vmmc-supply = <&vcc3v3_sd>;
	vqmmc-supply = <&vccio_sd>;
	status = "okay";
};
