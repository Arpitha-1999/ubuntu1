#include "sun50i-a64-olinuxiyes.dts"

/ {
	model = "Olimex A64-Olinuxiyes-eMMC";
	compatible = "olimex,a64-olinuxiyes-emmc", "allwinner,sun50i-a64";
};

&mmc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&mmc2_pins>;
	vmmc-supply = <&reg_dcdc1>;
	vqmmc-supply = <&reg_dcdc1>;
	bus-width = <8>;
	yesn-removable;
	cap-mmc-hw-reset;
	status = "okay";
};
