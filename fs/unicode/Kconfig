# UTF-8 yesrmalization
#
config UNICODE
	bool "UTF-8 yesrmalization and casefolding support"
	help
	  Say Y here to enable UTF-8 NFD yesrmalization and NFD+CF casefolding
	  support.

config UNICODE_NORMALIZATION_SELFTEST
	tristate "Test UTF-8 yesrmalization support"
	depends on UNICODE
	default n
