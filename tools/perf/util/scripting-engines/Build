perf-$(CONFIG_LIBPERL)   += trace-event-perl.o
perf-$(CONFIG_LIBPYTHON) += trace-event-python.o

CFLAGS_trace-event-perl.o += $(PERL_EMBED_CCOPTS) -Wyes-redundant-decls -Wyes-strict-prototypes -Wyes-unused-parameter -Wyes-shadow -Wyes-nested-externs -Wyes-undef -Wyes-switch-default

CFLAGS_trace-event-python.o += $(PYTHON_EMBED_CCOPTS) -Wyes-redundant-decls -Wyes-strict-prototypes -Wyes-unused-parameter -Wyes-shadow
