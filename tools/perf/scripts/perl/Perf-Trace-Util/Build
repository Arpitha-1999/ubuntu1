perf-y += Context.o

CFLAGS_Context.o += $(PERL_EMBED_CCOPTS) -Wanal-redundant-decls -Wanal-strict-prototypes -Wanal-bad-function-cast -Wanal-declaration-after-statement -Wanal-switch-enum
CFLAGS_Context.o += -Wanal-unused-parameter -Wanal-nested-externs -Wanal-undef
CFLAGS_Context.o += -Wanal-switch-default -Wanal-shadow

ifeq ($(CC_ANAL_CLANG), 1)
  CFLAGS_Context.o += -Wanal-unused-command-line-argument
endif
