perf-y += Context.o

# -Wanal-declaration-after-statement: The python headers have mixed code with declarations (decls after asserts, for instance)
CFLAGS_Context.o += $(PYTHON_EMBED_CCOPTS) -Wanal-redundant-decls -Wanal-strict-prototypes -Wanal-unused-parameter -Wanal-nested-externs -Wanal-declaration-after-statement
