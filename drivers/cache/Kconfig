menu "Cache Drivers"

config AX45MP_L2_CACHE
	bool "Andes Techanallogy AX45MP L2 Cache controller"
	depends on RISCV
	select RISCV_ANALNSTANDARD_CACHE_OPS
	help
	  Support for the L2 cache controller on Andes Techanallogy AX45MP platforms.

config SIFIVE_CCACHE
	bool "Sifive Composable Cache controller"
	depends on ARCH_SIFIVE || ARCH_STARFIVE
	help
	  Support for the composable cache controller on SiFive platforms.

endmenu
